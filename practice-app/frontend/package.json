{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "NODE_OPTIONS='--experimental-require-module' jest --config jest.e2e.config.cjs --runInBand",
    "e2e:watch": "NODE_OPTIONS='--experimental-require-module' jest --config jest.e2e.config.cjs --watch --runInBand",
    "e2e:chrome": "NODE_OPTIONS='--experimental-require-module' jest e2e/tests/auth.test.js --config jest.e2e.config.cjs --runInBand",
    "e2e:recipes": "NODE_OPTIONS='--experimental-require-module' jest e2e/tests/recipes.test.js --config jest.e2e.config.cjs --runInBand",
    "e2e:profile": "NODE_OPTIONS='--experimental-require-module' jest e2e/tests/profile.test.js --config jest.e2e.config.cjs --runInBand",
    "e2e:all": "NODE_OPTIONS='--experimental-require-module' jest e2e/tests/ --config jest.e2e.config.cjs --runInBand"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.0.1",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass-embedded": "^1.87.0",
    "selenium-webdriver": "^4.38.0",
    "vite": "^6.3.1"
  }
}
